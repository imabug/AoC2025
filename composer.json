{
    "name": "imabug/aoc2025",
    "description": "Code for Advent of Code 2025",
  "keywords": ["laravel", "laravel zero", "console", "cli", "Advent of Code"],
  "homepage": "https://v16.imablog.net/",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Eugene Mah",
            "email": "eugenemah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^12.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["aoc2025"]
}
